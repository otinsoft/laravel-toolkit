{
    "name": "otinsoft/laravel-toolkit",
    "description": "Common utilities for Laravel projects.",
    "homepage": "https://github.com/otinsoft/laravel-toolkit",
    "license": "MIT",
    "authors": [
        {
            "name": "Cretu Eusebiu",
            "email": "me@cretueusebiu.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "^3.4",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Otinsoft\\Toolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Otinsoft\\Toolkit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otinsoft\\Toolkit\\ToolkitServiceProvider"
            ]
        }
    }
}
